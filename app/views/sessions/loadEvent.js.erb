<% if !@events.nil? && @events.size > 0%>
	$('#last_event').val('<%= @events.first.id %>');
	
	<% @events.each do |event| %> 
		$('#EventList').prepend("<dd>Is public: <%= event.public %></dd>");
		$('#EventList').prepend("<dd>End date: <%= event.end_date %></dd>");
		$('#EventList').prepend("<dd>Start date: <%= event.start_date %></dd>");
		$('#EventList').prepend("<dd>Location: <%= event.location %></dd>");
		$('#EventList').prepend("<dd>Event: <%= event.name %></dd>");
		$('#EventList').prepend("<dt><b><%= event.user.f_name %></b></dt>");
		$('#EventList').prepend("<dl>");
	<% end %>
<% end %>